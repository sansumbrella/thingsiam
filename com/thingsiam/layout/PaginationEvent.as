package com.thingsiam.layout{	import flash.events.Event;	public class PaginationEvent extends Event	{		public static const BEFORE_PAGE_CHANGE:String = "beforePageChange";				public function PaginationEvent(type:String, bubbles:Boolean=false, cancelable:Boolean=false)		{			super(type, bubbles, cancelable);		}				override public function clone():Event		{			return new PaginationEvent( type, bubbles, cancelable );		}			}}